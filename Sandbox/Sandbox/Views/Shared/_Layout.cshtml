        <link href="~/stylesheets/jquery.tweet.css" rel="stylesheet" type="text" />
        <link href="~/stylesheets/navmenu.css" rel="stylesheet" type="text" />
        <link href="~/stylesheets/jquery.bxslider.css" rel="stylesheet" type="text" />
        <link href="~/stylesheets/isotope.css" rel="stylesheet" type="text" />
        <link href="~/stylesheets/intro02.css" rel="stylesheet" type="text" />
        <link href="~/stylesheets/main.css" rel="stylesheet" type="text" />
        <link href="~/stylesheets/main-responsive.css" rel="stylesheet" type="text" />
        <link href="~/stylesheets/main-retina.css" rel="stylesheet" type="text" />
        <link rel="~/stylesheet/less" type="text/css" href="less/color.less" />
        <link rel="~/stylesheet/less" type="text/css" href="less/fonts.less" />
        <script src="~/less/less-2.0.0.min.js"></script>
        <link href='http://fonts.googleapis.com/css?family=Raleway:400,300,700,900%7COpen+Sans:400,300,700,800%7CMontserrat:400,700%7CCrimson+Text:400,700,400italic,700italic%7CLekton:400,700' rel='stylesheet' type='text/css'>
