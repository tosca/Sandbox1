        <!--[if lt IE 9]>
      <script src="bootstrap/js/html5shiv.js"></script>
      <script src="bootstrap/js/respond.min.js"></script>
      <![endif]-->
    </head>
    <body class=" whitegray-bg">
        <div class="sticky-panel sticky-panel-top white-bg"></div>
        <div class="sticky-panel sticky-panel-right white-bg"></div>
        <div class="sticky-panel sticky-panel-bottom white-bg"></div>
        <header class="masthead visible-lg white-bg">
            <div class="main-logo-wrap">
                <img alt="" title="" src="~/images/logo.png" />
            </div>
            <div class="main-nav">
                <div class="mobile-toggle">
                    <span></span>
                    <span></span>
                    <span></span>
                </div>
            </div>
            <img alt="" title="" class="side-logo" src="~/images/side-logo.png" />
            <div class="nav-bio">
                <h4><span class="black color-bg font5">Natalie Macellaio 3D Metal Sculptor</span></h4>
            </div>
            <nav class="nav-item-wrap">
                <ul class="main-nav-menu main-nav-menu-effect">

                    <li class="trigger-sub-nav">
                        <a class="main-nav-link" href="~/Index">home</a>
                    </li>

                    <li class="trigger-sub-nav">
                        <a class="main-nav-link" href="#">work</a>
                        <div class="sub-nav">
                            <a href="~/Scribbles">Scribbles</a>
                            <a href="~/Kids-Rings">Kids-Rings</a>
                        </div>
                    </li>
                    <li class="trigger-sub-nav">
                        <a class="main-nav-link" href="#">collaborations</a>
                        <div class="sub-nav">
                            <a href="~/The-Mother-Load-Project">The Motherload Project</a>
                            <a href="#wannabe">Wannabe</a>
                            <a href="#petite">Petite Show</a>
                            <a href="#merger">The Merger</a>
                            <a href="#trestle">Trestle Designs</a>
                        </div>
                    </li>
                    <li class="trigger-sub-nav">
                        <a class="main-nav-link" href="#">about</a>
                        <div class="sub-nav">
                            <a href="#about">my bio</a>
                            <a href="#contact">contact me</a>
                        </div>
                    </li>
                </ul>
            </nav>

            <!-- social icons-->
            <ul class="social-wrap">
                <li><a href="#"><img alt="" title="" src="~/images/social/01.png" /></a></li>
            </ul>


        </header>

        <!-- mobile only navigation : starts -->
        <nav class="mobile-nav hidden-lg">
            <ul class="slimmenu">
                <li>
                    <a class="sub-collapser" href="/Index">home</a>
                </li>

                <li>
                    <a class="sub-collapser" href="">work</a>
                    <ul>
                        <li><a class="font2 black ease" href="/Scribbles">Scribbles</a></li>
                        <li><a class="font2 black ease" href="/Kids-Rings">Kids Rings</a></li>
                    </ul>
                </li>
                <li>
                    <a class="sub-collapser" href="">collaborations</a>
                    <ul>
                        <li><a class="font2 black ease" href="/The-Mother-Load-Project">The Motherload Project</a></li>
                        <li><a class="font2 black ease" href="#wannabe">Wannabe</a></li>
                        <li><a class="font2 black ease" href="#petite">Petite Show</a></li>
                        <li><a class="font2 black ease" href="#merger">The Merger</a></li>
                        <li><a class="font2 black ease" href="#trestle">Trestle Designs</a></li>
                    </ul>
                </li>
                <li>
                    <a class="sub-collapser" href="">about</a>
                    <ul>
                        <li><a class="font2 black ease" href="#about">my bio</a></li>
                        <li><a class="font2 black ease" href="#contact">contact me</a></li>
                    </ul>
                </li>
            </ul>
        </nav>
        <div class="mastwrap">
     
            @RenderBody()

            <footer>
                <div class="content-wrapper">
                    <div class="float-left">
                        <p>&copy; @DateTime.Now.Year - Natalie Macellaio - Metal Artist</p>
                    </div>
                </div>
            </footer>
        </div>

            <!-- @Scripts.Render("~/bundles/jquery")
        @RenderSection("scripts", required: false)-->

        <script src="~/javascripts/libs/jquery.min.js" type="text/javascript"></script>
        <script src="~/javascripts/libs/jquery.easing.1.3.js" type="text/javascript"></script>
        <script src="~/bootstrap/js/bootstrap.js" type="text/javascript"></script>
        <!--<script src="~/javascripts/libs/pace.preload.min.js" type="text/javascript"></script>-->
        <script src="~/javascripts/libs/retina.js" type="text/javascript"></script>
        <script src="~/javascripts/libs/device.min.js" type="text/javascript"></script>
        <script src="~/javascripts/libs/jquery.slimmenu.min.js" type="text/javascript"></script>
        <script src="~/javascripts/libs/classie.js" type="text/javascript"></script>
        <script src="~/javascripts/libs/jquery.touchSwipe.js" type="text/javascript"></script>
        <script src="~/javascripts/libs/equalheights.js" type="text/javascript"></script>
        <script src="~/javascripts/libs/isotope.js" type="text/javascript"></script>
        <script src="~/javascripts/libs/venobox.min.js" type="text/javascript"></script>
        <script src="~/javascripts/libs/jquery.bxslider.min.js" type="text/javascript"></script>
        <script src="~/javascripts/custom/navmenu-init.js" type="text/javascript"></script>
        <script src="~/javascripts/custom/equalheights-init.js" type="text/javascript"></script>
        <script src="~/javascripts/custom/isotope-init.js" type="text/javascript"></script>
        <script src="~/javascripts/custom/venobox-init.js" type="text/javascript"></script>
        <script src="~/javascripts/custom/main.js" type="text/javascript"></script>
        <script src="~/javascripts/custom/form-validation.js" type="text/javascript"></script>
        <script>
                $(document).ready(function () {
                    $('.bxslider').bxSlider({
                        preloadImages: 'visible',
                        touchEnabled: true,
                        responsive: true,
                        pager: false,
                        controls: true
                    });
                });
        </script>
</body> 
</html>
